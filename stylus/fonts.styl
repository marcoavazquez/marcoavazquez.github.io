@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?tsa4qn');
	src:url('../fonts/icomoon.eot?#iefixtsa4qn') format('embedded-opentype'),
		url('../fonts/icomoon.woff?tsa4qn') format('woff'),
		url('../fonts/icomoon.ttf?tsa4qn') format('truetype'),
		url('../fonts/icomoon.svg?tsa4qn#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
